<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module> <!-- Disabling "No grammar constraints" warning -->
<module rename-to='SoaArchiveClient'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />

	<!-- Inherit the default GWT style sheet. You can change -->
	<!-- the theme of your GWT application by uncommenting -->
	<!-- any one of the following lines. -->
	  <inherits name='com.google.gwt.user.theme.clean.Clean'/>
	  
	<!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->

	<!-- Other module inherits -->
	<!--  
	<inherits name='il.co.menora.ui.MenoraCommonClient' />
	-->
	<inherits name='com.google.gwt.activity.Activity' />
	<inherits name='com.google.gwt.resources.Resources' />
	<inherits name="com.vaadin.polymer.Elements"/>
	<inherits name='com.google.gwt.logging.Logging' />
	<inherits name="com.google.gwt.query.Query"/>
  	<inherits name="com.google.gwt.json.JSON" />
	<inherits name="org.fusesource.restygwt.RestyGWT"/>
	
	
	  <!-- Other module inherits                                      -->
	<stylesheet src='/archive/material.min.css' />
	<stylesheet src='/archive/dataTables.material.min.css' />
	<stylesheet src='/archive/dataTables.checkboxes.css' />
	
	
	
	
	<set-property name='gwt.logging.logLevel' value='INFO' />
	<set-property name='gwt.logging.enabled' value='TRUE' />

	<set-configuration-property name="CssResource.enableGss"
		value="true" />
    <set-configuration-property name="CssResource.conversionMode" value="strict" />
		

	<!-- Specify the app entry point class. -->
	<entry-point class='il.co.menora.soaarchive.client.app.SoaArchiveClient' />

	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />
</module>
