package il.co.menora.soaarchive.client.places;

import com.google.gwt.place.shared.PlaceTokenizer;
import com.google.gwt.place.shared.Prefix;

// Don't forget to add this place to ClientPlaceHistoryMapper!
public class OutgoingPlace extends AbstractPlace {
	@Prefix("outgoing")
	public static class Tokenizer implements PlaceTokenizer<OutgoingPlace> {
		@Override
		public String getToken(OutgoingPlace place) {
			return "";
		}

		@Override
		public OutgoingPlace getPlace(String token) {
			return new OutgoingPlace();
		}
	}
}
